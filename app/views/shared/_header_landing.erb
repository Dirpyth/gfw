<header id="headerView" class="layout-header">
  <div class="layout-header-top">
  	<div class="inner">
  		<%= render 'shared/navbar_landing' %>
  	</div>
  </div>
  <div class="layout-header-bottom">
  	<div class="inner">
  		<%= render 'shared/navbar2_landing' %>
  	</div>
  </div>
  <div class="layout-share-mobile">
    <a href="#" class="share-link" data-hideembed="1"><svg><use xlink:href="#shape-share"></use></svg></a>
    <% if content_for?(:headerMobile) %>
      <%= yield :headerMobile %>
    <% end %>
  </div>
</header>
