<!DOCTYPE html>
<html>
  <head>
    <%= render 'shared/meta' %>
    <%= render 'shared/links' %>

    <%= stylesheet_link_tag 'map', :media => 'all' %>

    <%= render 'shared/gfw_assets', page: 'map' %>
    <%= render 'shared/track' %>
    <%= render 'shared/favicons' %>

    <%= yield :head %>
    <%= yield :js %>
  </head>
  <body class="<%= "is-map-page is-#{action_name}-action" %>">
    <%= render "shared/header_landing" %>
    <%= yield %>
    <%= render 'shared/footer_landing' %>

    <%= render 'shared/icons/svg_icons' %>

    <%= requirejs_include_tag 'map' %>
  </body>
</html>
