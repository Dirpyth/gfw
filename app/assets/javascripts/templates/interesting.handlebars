  <div class="inner">
    <h2 class="section-title">You may find this interesting</h2>
    <div class="row">
      {{#each interesting}}
        <div class="col">
          <div class="card">
            <header>
              <h2>{{this.title}}</h2>
            </header>
            <div class="content">
              <p>{{this.description}}</p>
            </div>
            <footer>
              <a href="{{this.link}}" class="btn dark medium">Read More</a>
            </footer>
          </div>
        </div>        
      {{/each}}
    </div>
  </div>
