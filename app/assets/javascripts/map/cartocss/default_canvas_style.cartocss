marker-fill-opacity: 1;
marker-line-width: 0;
marker-placement: point;
marker-allow-overlap: true;
marker-width: [intensity];
marker-comp-op: src-over;

[zoom=7] {
 marker-allow-overlap: true;
  marker-width: [intensity]*1.15;
}
[zoom=8] {
 marker-allow-overlap: true;
  marker-width: [intensity]*1.3;
}
[zoom=9] {
 marker-allow-overlap: true;
  marker-width: [intensity]*1.5;
}
