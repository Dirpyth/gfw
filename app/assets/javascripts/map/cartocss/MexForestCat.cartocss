#mex_forest_zoning {
   polygon-opacity: 1;
   line-color: #FFF;
   line-width: 0;
   line-opacity: 1;
}

#mex_forest_zoning[categoria="No aplica"] {
   polygon-fill: #dfdfe0;
}
#mex_forest_zoning[categoria="Zonas de conservación y aprovechamiento restringido o prohibido"] {
   polygon-fill: #3E7BB6;
}
#mex_forest_zoning[categoria="Zonas de producción"] {
   polygon-fill: #bcdd64;
}
#mex_forest_zoning[categoria="Zonas de restauración"] {
   polygon-fill: #A53ED5;
}