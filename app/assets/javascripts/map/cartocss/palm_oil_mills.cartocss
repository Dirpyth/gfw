#oil_palm_mills {
   marker-fill-opacity: 1.0;
   marker-line-color: #FFF;
   marker-line-width: 0.5;
   marker-line-opacity: 1;
   marker-placement: point;
   marker-type: ellipse;
   marker-fill: #eadc15;
   marker-width: 4;
   marker-allow-overlap: true;

  [zoom > 2 ][zoom < 5]{
    marker-width: 5;
   }
  [zoom >= 5 ][zoom < 9]{
   marker-width: 7.5;
  }
  [zoom >= 9 ]{
   marker-width: 10;
  }

}
