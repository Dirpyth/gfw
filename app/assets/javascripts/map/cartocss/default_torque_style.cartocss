#layer {
  comp-op: lighter;
  marker-fill-opacity: 0.9;
  marker-line-color: #FFF;
  marker-line-width: 0;
  marker-line-opacity: 1;
  marker-type: ellipse;
  marker-width: 6;
  marker-fill: #0F3B82;
}

#layer[frame-offset=1] {
 marker-width:8;
 marker-fill-opacity:0.45; 
}

#layer[frame-offset=2] {
 marker-width:10;
 marker-fill-opacity:0.225; 
}"
