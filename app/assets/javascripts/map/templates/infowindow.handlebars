<div class="cartodb-popup">
  <a href="#close" class="cartodb-popup-close-button close"><svg><use xlink:href="#shape-close"></use></svg></a>
  <div class="cartodb-popup-content-wrapper {{#if content.data.analysis}}analysis-active{{/if}}">
    <div class="cartodb-popup-header">
      {{#if content.data.image}}
        {{#if content.data.name}}
          <h2>{{content.data.name}}</h2>
        {{/if}}
        {{!-- <div class="thumb-mask" style="background-image: url({{content.data.image}})"> --}}
          <div class="thumb-mask"><img src="{{content.data.image}}" onerror="this.style.display='none';"></div>
      {{/if}}
    </div>
    <div class="cartodb-popup-fixed">
      {{#if content.data.analysis}}
        <a style="cursor:pointer;" id="analyzeBtn" onclick="mps.publish('AnalysisTool/analyze-concession', [$(this).data('useid'), $(this).data('use'), $(this).data('wdpaid')]); ga('send', 'event', 'Map', 'Analysis', 'Analyze Protected Area');" class="analyse btn green uppercase" data-layer="{{content.data.id}}" data-useid="{{content.data.cartodb_id}}" data-use="{{content.data.tablename}}" data-wdpaid= "{{content.data.id}}">analyze</a>
      {{/if}}
    </div>
    <div class="cartodb-popup-content">
      <div class="cartodb-popup-content-inside">
        {{#unless content.data.image}}
          {{#if content.data.name}}
            <h2>{{content.data.name}}</h2>
          {{/if}}
        {{/unless}}

        {{#if content.data.local_name}}
          <h4>Local name:</h4>
          <p>{{content.data.local_name}}</p>
        {{/if}}
        {{#if content.data.desig_eng}}
          <h4>Legal designation:</h4>
          <p>{{content.data.desig_eng}}</p>
        {{/if}}
        {{#if content.data.ebaname}}
          <h4>Name:</h4>
          <p><a href="http://{{content.data.factsheet}}" target="_blank">{{content.data.ebaname}}</a></p>
        {{/if}}
        {{#if content.data.project_id}}
          <h4>Project ID:</h4>
          <p>{{content.data.project_id}}</p>
        {{/if}}

        {{#if content.data.id}}
          <h4>ID:</h4>
          <p>{{content.data.id}}</p>
        {{/if}}
        {{#if content.data.description}}
          <h4 class="no-float">Description:</h4>
          <p class="no-float">{{content.data.description}}</p>
        {{/if}}
        {{#if content.data.type}}
          <h4>Type:</h4>
          <p>{{content.data.type}}</p>
        {{/if}}
        {{#if content.data.status}}
          <h4>Status:</h4>
          <p>{{content.data.status}}</p>
        {{/if}}
        {{#if content.data.company}}
          <h4>Company:</h4>
          <p>{{content.data.company}}</p>
        {{/if}}
        {{#if content.data.permit_num}}
          <h4>Permit Num:</h4>
          <p>{{content.data.permit_num}}</p>
        {{/if}}
				{{#if content.data.project_na}}
          <h4>Project Name</h4>
          <p><a href="{{content.data.project_ur}}" target="_blank">{{content.data.project_na}}</a></p>
        {{/if}}
				{{#if content.data.project_pr}}
          <h4>Project Proponent :</h4>
          <p><a href="{{content.data.proponent_}}" target="_blank">{{content.data.project_pr}}</a></p>
        {{/if}}
        {{#if content.data.country}}
          <h4>Country:</h4>
          <p>{{content.data.country}}</p>
