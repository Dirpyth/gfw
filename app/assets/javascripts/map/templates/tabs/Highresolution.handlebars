<div class="tab-highresolution">
  <div class="highresolution-content">
    <h2>Imagery</h2>
    {{!-- <select data-placeholder="Select Provider" class="chosen-select default notranslate" id="hres-provider-select"><option></option></select> --}}

    <div data-slug="urthe" class="maptype">
      <h3>Urthecast<span class="onoffswitch"><span></span></span></h3>
      <span class="advanced-controls">Toggle Advanced Tools</span>
      <article class="advanced-options">      
        <section>
          <select data-placeholder="RGB (Red Green Blue)" class="chosen-select default notranslate" id="hres-filter-select" ><option></option></select>
        </section>
        <section>
          <span>maximum cloud cover percentage</span>

          <div class="range-box">
            <div class="dots"></div>
            <div id="progress-clouds" class="progress"></div>
            <input id="range-clouds" type="range" class='cloud' step="10" value="30">
          </div>

          {{!-- <input type="range" class='cloud' step="10" value="30"> --}}
        </section>
      </article>
      <section>
        <span>acquired date min</span> <input type=date min=2000-09-08 value="{{mindate}}" class="mindate">
      </section>
      <section>
        <span>acquired date max</span> <input type=date max={{today}} value="{{today}}" class="maxdate">
      </section>
    </div>
  </div>
</div>
